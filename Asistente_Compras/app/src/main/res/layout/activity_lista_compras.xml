<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tit_list_compras"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListaCompras">

    <ListView
        android:id="@+id/lista_compras"
        android:layout_width="449dp"
        android:layout_height="647dp"
        android:layout_below="@+id/textView13"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        android:layout_marginTop="21dp"
        android:choiceMode="none"
        android:clickable="false"
        android:longClickable="false"
        android:textFilterEnabled="false" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="411dp"
        android:layout_marginRight="411dp"
        android:layout_marginBottom="712dp"
        android:text="@string/txt_entrada" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="392dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="59dp"
        android:layout_marginRight="59dp"
        android:text="@string/txt_listaCompras"
        android:textColor="#000000"
        android:textSize="24sp" />
</RelativeLayout>